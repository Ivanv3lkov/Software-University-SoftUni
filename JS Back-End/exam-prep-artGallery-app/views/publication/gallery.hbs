<main>

    <section id="gallery">
        <h1>Gallery</h1>
        <article class="gallery-container">
            {{#each publications}}
                {{> publicationTemplate}}
            {{else}}

            <article class="no-available-publications">
                <h1>No publications created yet.</h1>
                <a href="/publications/create" class="create-pub">Create publication</a>
            </article>
            {{/each}}


        </article>
    </section>

</main>